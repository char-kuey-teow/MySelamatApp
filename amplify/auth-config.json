      "email"
    ],
    "userpoolClientReadAttributes": [
      "email"
    ],
    "userpoolClientLambdaRole": "myselamatapp_userpoolclient_lambda_role",
    "userpoolClientSetAttributes": false,
    "sharedId": "myselamatapp",
    "resourceName": "myselamatapp",
    "authSelections": "identityPoolAndUserPool",
    "useDefault": "default",
    "userPoolGroupList": [],
    "serviceName": "Cognito",
    "usernameCaseSensitive": false,
    "useEnabledMfas": true
  }
